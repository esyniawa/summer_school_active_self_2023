@article{holDifferentPopulationsNeurons2001,
  title = {Different Populations of Neurons Contribute to the Detection and Discrimination of Visual Motion},
  author = {Hol, Karel and Treue, Stefan},
  year = {2001},
  month = mar,
  journal = {Vision Research},
  volume = {41},
  number = {6},
  pages = {685--689},
  issn = {00426989},
  doi = {10.1016/S0042-6989(00)00314-X},
  urldate = {2023-03-03},
  abstract = {The signal-to-noise ratio of a direction-selective neuron for `detecting' visual motion is highest when the motion direction is close to the neuron's preferred direction. But because these neurons show a bell-shaped tuning for direction, they have the highest signal-to-noise ratio for `discriminating' the direction of motion when their preferred direction is off the direction to be discriminated. In this paper, we demonstrate with an adaptation paradigm that the visual system shows a corresponding task-specific ability to select neurons depending on whether it is performing a detection or a discrimination task, relying preferentially on different neuronal populations in the two tasks. Detection is based on neuronal populations tuned to the test direction, while direction discrimination is based on neurons preferring directions 40 \textendash{} 60\textdegree{} off the test direction. \textcopyright{} 2001 Elsevier Science Ltd. All rights reserved.},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/attention/Effiency target selection with attention/HOL & TREUE Different populations of neurons contribute to the detection and.pdf}
}

