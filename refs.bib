@article{brozzoliMultisensoryRepresentationSpace2014,
  title = {Multisensory {{Representation}} of the {{Space Near}} the {{Hand}}: {{From Perception}} to {{Action}} and {{Interindividual Interactions}}},
  shorttitle = {Multisensory {{Representation}} of the {{Space Near}} the {{Hand}}},
  author = {Brozzoli, Claudio and Ehrsson, H. Henrik and Farn{\`e}, Alessandro},
  year = {2014},
