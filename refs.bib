@article{chenShortLatencyCerebellar2014,
  title = {Short Latency Cerebellar Modulation of the Basal Ganglia},
  author = {Chen, Christopher H and Fremont, Rachel and {Arteaga-Bracho}, Eduardo E and Khodakhah, Kamran},
  year = {2014},
  month = dec,
  journal = {Nature Neuroscience},
  volume = {17},
